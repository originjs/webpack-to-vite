{
  "name": "webpack-to-vite",
  "version": "0.0.1",
  "description": "convert a webpack project to a vite project",
  "main": "bin/index.js",
  "bin": {
    "webpack-to-vite": "bin/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test-c": "jest-coverage",
    "lint": "eslint --ext .js,.ts src/",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/originjs/webpack-to-vite.git"
  },
  "keywords": [
    "vite",
    "webpack",
    "vue-cli"
  ],
  "author": "ygj6",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/originjs/webpack-to-vite/issues"
  },
  "homepage": "https://github.com/originjs/webpack-to-vite#readme",
  "devDependencies": {
    "@types/ejs": "^3.0.6",
    "@types/eslint-scope": "^3.7.0",
    "@types/jest": "^26.0.23",
    "@types/jscodeshift": "^0.11.0",
    "@types/webpack": "4.41.26",
    "@types/node": "^12.12.47",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2",
    "vite": "2.3.8",
    "vite-plugin-env-compatible": "^1.0.0",
    "vite-plugin-vue2": "^1.6.2",
    "vue-loader": "^15.9.7",
    "webpack": "4.46.0",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "@originjs/vue-sfc-ast-parser": "^1.0.1",
    "commander": "^7.2.0",
    "ejs": "^3.1.6",
    "jscodeshift": "^0.12.0",
    "webpack-chain": "4.12.1",
    "vue-eslint-parser": "^7.6.0",
    "webpack-merge": "4.2.2",
    "yargs": "^17.0.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
